{
  "manifest_version": 3,
  "name": "LeetCode Documentation Generator",
  "version": "1.0.0",
  "description": "Capture LeetCode submissions and generate formatted .docx documents for problem sets",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problems/*/submissions/*",
        "https://leetcode.com/submissions/detail/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "512": "icons/icon512.png"
  }
}
